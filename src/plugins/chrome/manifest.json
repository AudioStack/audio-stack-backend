{
  "manifest_version": 2,
  "name": "AudioStack",
  "description": "Handles audio players inside Chrome",
  "version": "1.0",
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistend": false
  },
  "content_scripts": [
    {
      "all_frames" : true,
      "match_about_blank": true,
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
